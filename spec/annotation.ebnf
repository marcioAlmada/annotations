annotation  ::= '@'(namespace delimiter)*(identifier) ((type |) value |)
identifier  ::= '[a-z] [A-Z] [0-9] _ -'
namespace   ::= ('\' |)('[a-z] [A-Z] _ -'(delimiter))*
delimiter   ::= '.' | '\'
type        ::= 'json' | 'string' | 'integer' | 'float' | '->'
value       ::= 'any unicode characters including \n'
